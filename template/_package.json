{
  "name": "<%= name %>",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "tslint src/**/*.ts{,x} --fix",
    "start": "NODE_ENV=development webpack-dev-server --hot --inline --config webpack.dev.js",
    "bundle": "NODE_ENV=production webpack --config webpack.prod.js",
    "build": "npm run clean && SOURCE_MAP=1 npm run bundle",
    "build:pre": "npm run clean && PRE=1 npm run bundle",
    "analysis": "ANALYSIS=1 npm run bundle",
    "stats": "NODE_ENV=production webpack --config webpack.prod.js --json > webpack-stats.json"
  },
  "author": "<%= username %> <<%= email %>>",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.3.12"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "autoprefixer": "^9.3.1",
    "babel-loader": "^8.0.4",
    "cache-loader": "^2.0.0",
    "css-loader": "^2.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.4",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.10.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "thread-loader": "^2.1.1",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.1.6",
    "webpack": "^4.25.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
